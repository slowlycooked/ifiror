<div class="row">
  <div class="col-xs-12 col-lg-4">
    <%= form_for(resource, as: resource_name, class: "form-signin", url: session_path(resource_name)) do |f| %>
        <div class="field">
          <%= f.label '用户名' %><br/>
          <%= f.text_field :login, class: "form-control", autofocus: true %>
        </div>
        <div class="field">
          <%= f.label '密码' %><br/>
          <%= f.password_field :password, class: "form-control", autocomplete: "off" %>
        </div>
        <div class="actions">
          <%= f.submit "登录", class: "btn btn-lg btn-primary btn-block" %>
        </div>
    <% end %>

  </div>
</div>
